<script type="text/javascript">
function idcheck() {
	if (document.frm.id.value == "") {
		alert('아이디를 입력하여 주십시오.');
		document.frm.id.focus();
		return;
	}
	var url = "TalentServlet?command=id_check_form&id=" + document.frm.id.value;
	window.open(url, "_blank_1",
			"toolbar=no, menubar=no, scrollbars=yes, resizable=no, width=350, height=200, top=300, left=300");
}

function go_save() {
	if (document.frm.id.value == "") {
		alert("아이디를 입력해 주세요.");
		document.frm.id.focus();
	} else if (document.frm.id.value != document.frm.reid.value) {
		alert("아이디 중복확인을 완료해 주세요.");
		document.frm.id.focus();
	} else if (document.frm.pwd.value == "") {
		alert("비밀번호를 입력해 주세요.");
		document.frm.pwd.focus();
	} else if ((document.frm.pwd.value != document.frm.pwdCheck.value)) {
		alert("비밀번호가 일치하지 않습니다.");
		document.frm.pwd.focus();
	} else if (document.frm.name.value == "") {
		alert("이름을 입력해 주세요.");
		document.frm.name.focus();
	} else if (document.frm.email.value == "") {
		alert("이메일을 입력해 주세요.");
		document.frm.email.focus();
	} else if (document.frm.okon1[1].checked == true) {
		alert('약관에 동의하셔야만 합니다.');
	} else {
		document.frm.action = "TalentServlet?command=join";
		document.frm.submit();
	}
}
</script>

